{
  "name": "TradeFy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./ -o ./dist",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify app/index.ts",
    "start:prod": "ts-node -r tsconfig-paths/register ./dist/app/index.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "knex:seed": "knex --knexfile knexfile.ts seed:run",
    "knex:migrate": "knex migrate:latest --knexfile knexfile.ts migratte:latest"
  },
  "dependencies": {
    "@types/config": "^0.0.38",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "mongodb": "^3.6.4",
    "pg": "^8.5.1",
    "swagger-autogen": "^1.2.5",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/mongodb": "^3.6.9",
    "@types/node": "^14.14.20",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "2.2.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write ."
    ]
  }
}
